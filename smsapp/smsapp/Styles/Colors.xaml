<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:smsapp">
    <Color x:Key="AppBackgroundColor">#ffffff</Color>
    <SolidColorBrush x:Key="AppBackgroundBrush" Color="{StaticResource AppBackgroundColor}"></SolidColorBrush>

    <Color x:Key="TitleBarColor">#004D40</Color>
    <SolidColorBrush x:Key="TitleBarBrush" Color="{StaticResource TitleBarColor}"></SolidColorBrush>

    <Color x:Key="MainTextColor">#E0F2F1</Color>
    <SolidColorBrush x:Key="MainTextBrush" Color="{StaticResource MainTextColor}"></SolidColorBrush>

    <Color x:Key="MainControlColor">#004D40</Color>
    <SolidColorBrush x:Key="MainControlBrush" Color="{StaticResource MainControlColor}"></SolidColorBrush>

    <Color x:Key="LighterControlColor">#009688</Color>
    <SolidColorBrush x:Key="LighterControlBrush" Color="{StaticResource LighterControlColor}"></SolidColorBrush>

    <Color x:Key="ActiveControlColor">#00BFA5</Color>
    <SolidColorBrush x:Key="ActiveControlBrush" Color="{StaticResource ActiveControlColor}"></SolidColorBrush>

    <Color x:Key="TransparentColor">#00b0f0</Color>
    <SolidColorBrush x:Key="TransparentBrush" Color="Black" Opacity="0.5"></SolidColorBrush>
</ResourceDictionary>